generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum panel_user_type {
  ADMIN
  EMPLOYEE
}

model panel_user {
  panel_user_id   String          @id
  email           String          @unique
  fullname        String
  profile_picture String
  role            panel_user_type

  created_at DateTime @default(now())
}
